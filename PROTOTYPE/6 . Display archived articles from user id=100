SELECT * FROM article_1 WHERE date_article_1 = (SELECT GREATEST(MAX(date_article_1), CURDATE()) FROM article_1);
UPDATE article_1 SET archived_1 = 1 WHERE id_WWW_user_1 BETWEEN 100 AND 106 ;

