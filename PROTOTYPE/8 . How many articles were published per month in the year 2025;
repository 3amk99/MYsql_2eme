SELECT MONTH(date_article_1) AS month_number , COUNT(*) AS number_of_articles FROM article_1 WHERE YEAR(date_article_1) = 2025 GROUP BY MONTH(date_article_1) ORDER BY MONTH(date_article_1) ;
